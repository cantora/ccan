#include <stdio.h>
#include <string.h>
#include "config.h"

/**
 * tok_itr - iterate over tokens in a string
 *
 * The tok_itr module iterates for tokens separated by 
 * a delimiter character in a null terminated string.
 *
 * Example:
 *  #include <stdio.h>
 *  #include <ccan/tok_itr/tok_itr.h>
 *
 *  int main(int argc, char *argv[]) {
 *    size_t len;
 *    char *token;
 *    struct tok_itr itr;
 *    char str[] = "/bin:/usr/bin:/usr/local/sbin";
 *
 *    for(tok_itr_init(&itr, str, ':'); !tok_itr_end(&itr); tok_itr_next(&itr) ) {
 *      len = (tok_itr_val_len(&itr) + 1);
 *      token = malloc(sizeof(char) * len);
 *      tok_itr_val(&itr, token, len);
 *      printf("token = %s\n", token);
 *      free(token);
 *    }
 *
 *    return 0;
 *  }
 * License: LGPL (v3 or any later version)
 * Author: anthony cantor <ar.cantor@gmail.com>
 */
int main(int argc, char *argv[])
{
	if (argc != 2)
		return 1;

	if (strcmp(argv[1], "depends") == 0) {
		return 0;
	}

	return 1;
}
